@model ContactCreateDto
@{
    ViewData["Title"] = "Yeni";
}
<div class="ia_admin_edit">
    <form method="post">
        <div class="ia_admin_form">
            <p class="ia_title">Yeni iletişim bilgileri ekle</p>
            <input class="ia_admin_input" asp-for="Title" placeholder="İl İlçe" />
            <textarea class="ia_admin_input" asp-for="Description" placeholder="Adres"></textarea>
            <input class="ia_admin_input" asp-for="Mail" placeholder="E posta" />
            <input class="ia_admin_input" asp-for="Phone" placeholder="Telefon" />
@*             <input class="ia_admin_input" asp-for="Location" placeholder="Konum" /> *@
            <button type="submit" class="ia_button_blue">Ekle</button>
        </div>
    </form>
</div>