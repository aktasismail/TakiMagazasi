@model List<About>
@{
    ViewData["Title"] = "Index";
}
<div class="ia_admin_list">
    <div class="ia_admin_list_header">
        <p class="ia_title">Hakkımızda</p>
        <a class="ia_new_button" href="About/CreateAbout/">Yeni Ekle</a>
    </div>
    <div>
        <table class="ia_admin_list_table">
            <tr>
                <th>Başlık</th>
                <th>Açıklama</th>
                <th>Düzenleme</th>
                <th>Silme</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Title</td>
                    <td>@item.Description</td>
                    <td><a href="/About/DeleteAbout/@item.AboutId" class="ia_button_remove">Sil</a></td>
                    <td><a href="/About/UpdateAbout/@item.AboutId" class="ia_button_success">Güncelle</a></td>
                </tr>
            }
        </table>
    </div>
</div>